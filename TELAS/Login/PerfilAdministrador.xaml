                <TextBox x:Name="foneadmtxt" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Margin="109,175,0,0" IsReadOnly="True"/>
                <Button Content="Voltar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="113" Margin="11,227,0,0" Background="#FFFCBAFF" Click="Button_Voltar"/>
                <Image x:Name="fotoadm" HorizontalAlignment="Left" Height="144" VerticalAlignment="Top" Width="144" Margin="307,21,0,0"/>
                <Image Margin="307,34,35,96" Source="administrador.jpg" Stretch="Fill"/>

            </Grid>
        </GroupBox>

    </Grid>
</Window>
