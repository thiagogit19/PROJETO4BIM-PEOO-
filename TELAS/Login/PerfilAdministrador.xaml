                <TextBox x:Name="nomeadmtxt" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Margin="109,12,0,0" IsReadOnly="True"/>
                <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Nascimento:" VerticalAlignment="Top" Margin="11,118,0,0" Height="23" Width="80" FontSize="14"/>
                <TextBox x:Name="nascadmtxt" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Margin="109,118,0,0" IsReadOnly="True"/>
